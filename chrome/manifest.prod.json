{
  "version": "0.0.0",
  "name": "react-chrome-extension-example",
  "manifest_version": 2,
  "description": "Example for react-chrome-extension-boilerplate",
  "browser_action": {
    "default_title": "React Chrome Extension Example",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-16-not-connected.png",
    "48": "img/icon-48-not-connected.png",
    "128": "img/icon-128-not-connected.png"
  },
  "web_accessible_resources": [
    "inject.html"
  ],
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*"],
      "exclude": [],
      "js": ["/js/popup.bundle.js"],
      "css": ["scripts/app.css"]
    }
  ],
  "permissions": [ "contextMenus", "tabs", "storage", "https://wikiweb.org/*" ],
  "content_security_policy": "default-src 'self'; script-src 'self'; style-src * 'unsafe-inline'; connect-src 'self' https://www.google-analytics.com https://wikiweb.org; img-src 'self' http://galacticapp.s3.amazonaws.com https://galacticapp.s3.amazonaws.com data;"
}
